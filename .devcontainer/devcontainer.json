{
    "name": "WECAN 2",
    "image": "mcr.microsoft.com/devcontainers/javascript-node",
    "features": {
        "ghcr.io/itsmechlark/features/postgresql:1": {}
    },
  
    "postCreateCommand": "npm install",
  
    "waitFor": "postCreateCommand",
  
    "customizations": {
        "codespaces": {
            "openFiles": ["README.md"]
        },
        "vscode": {
            "extensions": ["svelte.svelte-vscode"]
        }
    }
}
